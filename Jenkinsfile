pipeline 
{
         agent any
         stages 
        {
                 stage('One') 
                 {
                            steps {    echo 'Development Stage...'  }
                 }
                 stage('Two') 
                 {
                           steps {  echo('Sample testing of pipeline 3')   }
                 }
                 stage('Three') 
                 {
                          steps {   echo 'Thanks for using Jenkins Pipeline' }
                 }
       }
}
